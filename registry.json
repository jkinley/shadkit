{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "acme",
  "homepage": "https://acme.com",
  "items": [
    {
      "name": "autogrid",
      "type": "registry:ui",
      "css": {
        "@layer components": {
          ".autogrid": {
            "display": "grid",
            "grid-template-columns": "repeat(var(--mode, auto-fill), minmax(var(--col-width, 45ch), 1fr))",
            "gap": "var(--gap, 1rem)"
          }
        }
      },
      "files": [
        {
          "path": "registry/ui/autogrid.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "alert",
      "type": "registry:ui",
      "files": [
        {
          "path": "registry/ui/alert.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "card",
      "type": "registry:ui",
      "files": [
        {
          "path": "registry/ui/card.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "dashgrid",
      "type": "registry:block",
      "title": "Dashgrid",
      "description": "A grid layout for displaying dashboard items.",
      "registryDependencies": [
        "https://shadkit.vercel.app/r/card.json"
      ],
      "files": [
        {
          "path": "registry/blocks/dashgrid/dashgrid.tsx",
          "type": "registry:page",
          "target": "~/src/routes/dashgrid.tsx"
        }
      ]
    }
  ]
}